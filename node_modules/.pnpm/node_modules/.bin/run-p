#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/and1zzz/Desktop/test/MONOREPO/node_modules/.pnpm/npm-run-all2@6.1.2/node_modules/npm-run-all2/bin/run-p/node_modules:/Users/and1zzz/Desktop/test/MONOREPO/node_modules/.pnpm/npm-run-all2@6.1.2/node_modules/npm-run-all2/bin/node_modules:/Users/and1zzz/Desktop/test/MONOREPO/node_modules/.pnpm/npm-run-all2@6.1.2/node_modules/npm-run-all2/node_modules:/Users/and1zzz/Desktop/test/MONOREPO/node_modules/.pnpm/npm-run-all2@6.1.2/node_modules:/Users/and1zzz/Desktop/test/MONOREPO/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/and1zzz/Desktop/test/MONOREPO/node_modules/.pnpm/npm-run-all2@6.1.2/node_modules/npm-run-all2/bin/run-p/node_modules:/Users/and1zzz/Desktop/test/MONOREPO/node_modules/.pnpm/npm-run-all2@6.1.2/node_modules/npm-run-all2/bin/node_modules:/Users/and1zzz/Desktop/test/MONOREPO/node_modules/.pnpm/npm-run-all2@6.1.2/node_modules/npm-run-all2/node_modules:/Users/and1zzz/Desktop/test/MONOREPO/node_modules/.pnpm/npm-run-all2@6.1.2/node_modules:/Users/and1zzz/Desktop/test/MONOREPO/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../npm-run-all2/bin/run-p/index.js" "$@"
else
  exec node  "$basedir/../npm-run-all2/bin/run-p/index.js" "$@"
fi
